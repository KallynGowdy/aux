{
    "name": "@casual-simulation/gpio",
    "private": true,
    "version": "0.0.1",
    "description": "A library that contains operations needed to control the input and output of GPIO pins.",
    "keywords": [
        "aux"
    ],
    "author": "Casual Simulation, Inc.",
    "homepage": "https://casualsimulation.com/",
    "license": "MIT",
    "main": "index.ts",
    "types": "index.d.ts",
    "module": "index.ts",
    "directories": {
        "lib": "."
    },
    "files": [
        "/README.md",
        "/LICENSE.txt",
        "**/*.ts",
        "**/*.ts.map",
        "**/*.d.ts"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/casual-simulation/casualos.git"
    },
    "scripts": {
        "build": "npm run webpack",
        "webpack": "webpack --config webpack.config.js",
        "package": "docker build -t casualsimulation/gpio -f Dockerfile .",
        "start": "node dist/main.ts"
    },
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/casual-simulation/casualos/issues"
    },
    "dependencies": {
        "onoff": "6.0.0"
    }
}
