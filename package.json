{
  "name": "process-for-teams",
  "version": "1.0.0",
  "description": "The repository for the Process for Teams project.",
  "scripts": {
    "start": "node SO4/dist/main.js",
    "watch": "concurrently \"npm:watch:server\" \"npm:watch:client\" \"nodemon SO4/dist/main.js\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config WebClient/webpack.prod.js",
    "build:client:profile": "webpack --config WebClient/webpack.prod.js --profile --json > bundle_stats.json && webpack-bundle-analyzer bundle_stats.json",
    "build:server": "webpack --config SO4/webpack.config.js",
    "watch:server": "webpack --config SO4/webpack.config.js --watch",
    "watch:client": "webpack --config WebClient/webpack.dev.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeticgi/process-for-teams.git"
  },
  "author": "YETi CGI",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/yeticgi/process-for-teams/issues"
  },
  "homepage": "https://github.com/yeticgi/process-for-teams#readme",
  "private": true,
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/express": "^4.16.0",
    "@types/http-proxy": "^1.16.2",
    "@types/pify": "^3.0.2",
    "@types/socket.io": "^2.1.0",
    "@types/three": "^0.93.10",
    "@types/uuid": "^3.4.4",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "vue-loader": "^15.4.2",
    "vue-svg-loader": "^0.11.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "browserfs": "^1.4.3",
    "concurrently": "^4.0.1",
    "es6-promise": "^4.2.5",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "http-proxy": "^1.17.0",
    "isomorphic-git": "^0.39.4",
    "socket.io": "^2.1.1",
    "three": "^0.98.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-router": "^3.0.1"
  }
}
