{
    "name": "root",
    "private": true,
    "version": "0.3.0",
    "scripts": {
        "start": "node ./src/aux-server/server/dist/main.js",
        "bootstrap": "lerna bootstrap --hoist",
        "watch": "lerna exec --parallel -- npm run watch",
        "watch:player": "lerna exec --parallel -- npm run watch:player",
        "build": "lerna exec -- npm run build",
        "build:docs": "lerna exec -- npm run build:docs",
        "test:watch": "npm run clean && jest --watchAll",
        "test": "npm run clean && jest --verbose",
        "test:ci": "npm run clean && jest --ci --reporters=default --reporters=jest-junit",
        "test:coverage": "npm test -- --coverage --reporters=default --reporters=jest-html-reporter",
        "clean": "lerna exec -- npm run clean",
        "debug:server:chrome": "node --inspect src/aux-server/server/dist/main.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
