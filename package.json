{
    "name": "root",
    "private": true,
    "version": "0.3.0",
    "scripts": {
        "start": "node ./src/aux-server/server/dist/main.js",
        "bootstrap": "lerna bootstrap --hoist",
        "watch": "npm run build:libs && concurrently --names \"Libs,Server\" \"npm run watch:libs\" \"npm run watch:server\"",
        "watch:player": "npm run build:libs && concurrently \"npm run watch:libs\" \"npm run watch:server:player\"",
        "watch:server": "lerna exec --no-prefix --scope @casual-simulation/aux-server -- npm run watch",
        "watch:server:player": "lerna exec --no-prefix --scope @casual-simulation/aux-server -- npm run watch:player",
        "watch:libs": "tsc --build --watch",
        "rebuild": "npm run clean && npm run build",
        "build": "npm run build:libs && npm run build:server",
        "build:tar": "npm run build && npm run tar",
        "build:docker": "npm run build && npm run package:docker",
        "build:docker:arm32": "npm run build:tar && npm run package:docker:arm32",
        "build:libs": "tsc --build",
        "build:server": "lerna exec --scope @casual-simulation/aux-server -- npm run build",
        "build:docs": "npm run clean && lerna exec -- npm run build:docs",
        "build:profile": "lerna exec --scope @casual-simulation/aux-server -- npm run build:profile",
        "package:docker": "docker build -t casualsimulation/aux .",
        "package:docker:arm32": "docker build -t casualsimulation/aux-arm32 -f Dockerfile.arm32 .",
        "test:watch": "npm run clean && jest --watchAll",
        "test": "npm run clean && jest --verbose",
        "test:ci": "npm run clean && jest --ci --reporters=default --reporters=jest-junit",
        "test:coverage": "npm test -- --coverage --reporters=default --reporters=jest-html-reporter",
        "clean": "gulp clean",
        "debug:server:chrome": "node --inspect src/aux-server/server/dist/main.js",
        "tar": "node ./script/make-tar.js",
        "view:server:profile": "webpack-bundle-analyzer src/aux-server/server_bundle_stats.json",
        "view:web:profile": "webpack-bundle-analyzer src/aux-server/web_bundle_stats.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@types/base64-js": "^1.2.5",
        "@types/bn.js": "^4.11.4",
        "@types/chai": "^4.1.7",
        "@types/d3": "^5.7.1",
        "@types/estraverse": "^0.0.6",
        "@types/express": "^4.16.0",
        "@types/http-proxy": "^1.16.2",
        "@types/jest": "^23.3.11",
        "@types/lodash": "^4.14.123",
        "@types/lru-cache": "^4.1.1",
        "@types/mongodb": "^3.1.25",
        "@types/pify": "^3.0.2",
        "@types/redis": "2.8.12",
        "@types/sharp": "0.22.1",
        "@types/sinon": "^7.0.13",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "@types/uuid": "^3.3.2",
        "@types/vue-color": "^2.4.1",
        "@types/useragent": "^2.1.1",
        "babel-loader": "^8.0.4",
        "braces": "^2.3.2",
        "chai": "^4.2.0",
        "clean-webpack-plugin": "^1.0.0",
        "concurrently": "^4.0.1",
        "css-loader": "^1.0.1",
        "del": "^3.0.0",
        "exports-loader": "^0.7.0",
        "file-loader": "^2.0.0",
        "gulp": "^4.0.0",
        "handlebars": "^4.1.2",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "imports-loader": "^0.8.0",
        "jest": "^24.1.0",
        "jest-html-reporter": "^2.5.0",
        "jest-junit": "^6.3.0",
        "jest-raw-loader": "^1.0.1",
        "js-yaml": "^3.13.1",
        "lerna": "^3.13.4",
        "mini-css-extract-plugin": "^0.5.0",
        "nodemon": "^1.18.6",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "precise-commits": "^1.0.2",
        "prettier": "^1.17.0",
        "pretty-quick": "^1.10.0",
        "raw-loader": "^1.0.0",
        "sinon": "^7.2.4",
        "source-map-loader": "^0.2.4",
        "socket.io-mock": "1.2.3",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.1",
        "ts-jest": "^24.0.0",
        "ts-loader": "6.0.3",
        "typedoc": "^0.14.2",
        "typescript": "^3.4.1",
        "vue-loader": "^15.4.2",
        "vue-svg-loader": "^0.11.0",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.25.1",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.4",
        "webpack-node-externals": "^1.7.2",
        "worker-loader": "^2.0.0",
        "tar": "4.4.8",
        "circular-dependency-plugin": "5.0.2"
    },
    "dependencies": {}
}
