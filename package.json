{
  "name": "process-for-teams",
  "version": "1.0.0",
  "description": "The repository for the Process for Teams project.",
  "scripts": {
    "start": "node SO4/dist/main.js",
    "watch": "concurrently \"npm:watch:server\" \"npm:watch:client\" \"nodemon SO4/dist/main.js\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config WebClient/webpack.prod.js",
    "build:client:profile": "webpack --config WebClient/webpack.prod.js --profile --json > bundle_stats.json && webpack-bundle-analyzer bundle_stats.json",
    "build:server": "webpack --config SO4/webpack.config.js",
    "watch:server": "webpack --config SO4/webpack.config.js --watch",
    "watch:client": "webpack --config WebClient/webpack.dev.js --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeticgi/process-for-teams.git"
  },
  "author": "YETi CGI",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/yeticgi/process-for-teams/issues"
  },
  "homepage": "https://github.com/yeticgi/process-for-teams#readme",
  "private": true,
